{
  "name": "vue-plugin",
  "version": "0.0.1",

  "scripts": {
    "clean": "rimraf prod",
    "dev": "webpack --watch --config ./build/webpack.conf.dev.js",
    "build": "npm run clean && webpack --config ./build/webpack.conf.prod.js",
    "start": "webpack-dev-server --hot --open"
  },

  "dependencies": {
    "webpack": "2.4.1",
    "webpack-merge": "4.1.0",

    "babel-loader": "7.0.0",
    "babel-core": "6.24.1",
    "babel-preset-env": "1.4.0",
    "regenerator-runtime": "0.10.3",

    "vue-loader": "12.0.0",
    "vue-template-compiler": "2.5.13",
    "css-loader": "0.26.1",
    "sass-loader": "6.0.3",
    "node-sass": "4.5.2",
    "autoprefixer": "6.7.7",

    "html-webpack-plugin": "2.28.0",

    "url-loader": "0.5.8",
    "file-loader": "0.11.1",

    "vue": "2.5.13",
    "vue-router": "3.0.1",
    "vuex": "3.0.1"
  },
  "devDependencies": {
    "eslint": "3.19.0",
    "eslint-plugin-html": "2.0.1",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.3",
    "webpack-dev-server": "^2.9.1"
  }
}
