{
  "name": "vue-plugin",
  "version": "0.0.1",
  "scripts": {
    "clean": "rimraf prod",
    "build:dev": "webpack --watch --config ./build/webpack.conf.dev.js",
    "build": "npm run clean && webpack --config ./build/webpack.conf.prod.js",
    "dev": "webpack-dev-server --hot --open --config ./build/webpack.conf.dev.js",
    "lint": "eslint .",
    "precommit": [
      "lint"
    ]
  },
  "dependencies": {
