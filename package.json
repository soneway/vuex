    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "0.26.1",
    "file-loader": "0.11.1",
    "html-webpack-plugin": "2.28.0",
    "node-sass": "4.5.2",
    "sass-loader": "6.0.3",
    "url-loader": "0.5.8",
    "vue": "2.5.13",
    "vue-loader": "12.0.0",
    "vue-router": "3.0.1",
    "vue-template-compiler": "2.5.13",
